{
  "rules": {
    "no-unused-variable": [false, "react"],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "array-type": [true, "array"],
    "jsx-no-lambda":false,
    "no-console": false,
    "interface-name": [
      true,
      "never-prefix"
    ]
  },
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
